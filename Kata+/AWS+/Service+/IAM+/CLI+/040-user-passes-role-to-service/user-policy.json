{
    "Version": "2012-10-17", 
    "Statement": [
        {
            "Effect": "Allow", 
            "Action": ["lambda:CreateFunction"], 
            "Resource": "*"
        }, 
        {
            "Effect": "Allow", 
            "Action": "iam:PassRole", 
            "Resource": "*", 
            "Condition": {
                "StringEquals": {"iam:PassedToService": "lambda.amazonaws.com"}
            }
        }
    ]
}
