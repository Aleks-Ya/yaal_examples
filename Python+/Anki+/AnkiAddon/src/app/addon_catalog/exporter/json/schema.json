{
  "$schema": "http://json-schema.org/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "title": {
        "type": "string"
      },
      "addon_page": {
        "type": "string"
      },
      "rating": {
        "type": "integer"
      },
      "update_date": {
        "type": "string"
      },
      "versions_str": {
        "type": "string"
      },
      "versions": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "min_version": {
              "type": "string"
            },
            "max_version": {
              "type": [
                "null",
                "string"
              ]
            },
            "updated": {
              "type": "string"
            }
          },
          "required": [
            "max_version",
            "min_version",
            "updated"
          ]
        }
      },
      "anki_forum_url": {
        "type": [
          "null",
          "string"
        ]
      },
      "github": {
        "anyOf": [
          {
            "type": "null"
          },
          {
            "type": "object",
            "properties": {
              "user": {
                "type": "string"
              },
              "repo": {
                "type": "string"
              },
              "languages": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "stars": {
                "type": "integer"
              },
              "last_commit": {
                "type": [
                  "null",
                  "string"
                ]
              },
              "links": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "url": {
                      "type": "string"
                    },
                    "user": {
                      "type": "string"
                    },
                    "repo": {
                      "type": [
                        "null",
                        "string"
                      ]
                    }
                  },
                  "required": [
                    "repo",
                    "url",
                    "user"
                  ]
                }
              },
              "action_count": {
                "type": [
                  "integer",
                  "null"
                ]
              },
              "tests_count": {
                "type": [
                  "integer",
                  "null"
                ]
              }
            },
            "required": [
              "action_count",
              "languages",
              "last_commit",
              "links",
              "repo",
              "stars",
              "tests_count",
              "user"
            ]
          }
        ]
      },
      "links": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "likes": {
        "type": "integer"
      },
      "dislikes": {
        "type": "integer"
      }
    },
    "required": [
      "addon_page",
      "anki_forum_url",
      "dislikes",
      "github",
      "id",
      "likes",
      "links",
      "rating",
      "title",
      "update_date",
      "versions",
      "versions_str"
    ]
  }
}
