GET    /person                  controllers.PlaySlickLiquibasePersonController.getPersons
POST   /person                  controllers.PlaySlickLiquibasePersonController.createPerson
DELETE /person                  controllers.PlaySlickLiquibasePersonController.deletePerson

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file           controllers.Assets.versioned(path="/public", file: Asset)
